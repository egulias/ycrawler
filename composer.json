{
    "name": "yunait/daily-cli",
    "homepage": "http://github.com/yunait/daily-cli",
    "type": "library",
    "repositories": [
        {"type": "vcs", "url": "git@github.com:yunait/core.git"},
        {"type": "vcs", "url": "git@github.com:yunait/core-delivery.git"},
        {"type": "vcs", "url": "git@github.com:yunait/core-business.git"},
        {"type": "vcs", "url": "git@github.com:yunait/core-daily.git"},
        {"type": "vcs", "url": "git@github.com:yunait/string-forge.git"}
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "ext-mongo": "*",
        "ext-yaml": "*",
        "ext-redis": "*",
        "ext-stats": "*",
        "yunait/core": "dev-master",
        "yunait/core-daily": "dev-master",
        "yunait/core-business": "dev-master",
        "aws/aws-sdk-php": "2.*",
        "aza/thread": "~1.0",
        "guzzle/guzzle": "4.*"

    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "psr-0": {
            "Daily\\CLI\\": "src/",
            "Daily\\CLI\\Commands\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": "vendor/yunait/core/bin/console core:models",
        "post-install-cmd": "vendor/yunait/core/bin/console core:models"
    }
}
